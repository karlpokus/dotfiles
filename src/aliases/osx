# colored output -p adds / to dirs -F adds /@| to stuff
alias ls='ls -G'
# chrome headless
alias chrome="/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome"
# tldr <cmd>
alias tldr='bash ~/dev/tldr/tldr.sh'
# ssn
alias ssn='~/dev/pernr/lib/rand.js | pbcopy; echo -e "$(pbpaste) copied to clipboard"'
# list hosts
alias hosts='less ~/.ssh/config'
# ip
alias ip-lan='ipconfig getifaddr en0'
# gopkg
alias gopkg='~/golang/src/github.com/karlpokus/gopkg/gopkg'
# http server status
alias status='~/dev/http-status-code/request.sh'
# url-stash
alias urst='~/dev/url-stash/urst.sh ~/Dropbox/doctrin/urls'
# flow-up
alias flow-up='~/doc/flow-up/up.sh'
# note: json2table is in /usr/local/bin

